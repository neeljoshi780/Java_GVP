import java.util.*;

public class P2 {
    public static void main(String[] args) {

        System.out.println("Before Sorting:");
        for (int i = 0; i < args.length; i++) {
            System.out.print(args[i] + ",");
        }

        System.out.println("\nAfter Sorting:");
        Arrays.sort(args);
        for (int i = 0; i < args.length; i++) {
            System.out.print(args[i] + ",");
        }
    }
}
